{
  "id": null,
  "uid": "unifi-network",
  "title": "UniFi Network",
  "tags": [
    "unifi",
    "network"
  ],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "templating": {
    "list": []
  },
  "panels": [
    {
      "type": "stat",
      "title": "Devices Up",
      "datasource": "Prometheus",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "sum(unpoller_unifi_device_up)",
          "legendFormat": "devices"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "decimals": 0
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "textMode": "auto"
      }
    },
    {
      "type": "stat",
      "title": "Clients",
      "datasource": "Prometheus",
      "gridPos": {
        "x": 6,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "sum(unpoller_unifi_device_num_sta)",
          "legendFormat": "clients"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "decimals": 0
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "textMode": "auto"
      }
    },
    {
      "type": "stat",
      "title": "CPU Usage (avg)",
      "datasource": "Prometheus",
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "avg(unpoller_unifi_device_cpu_usage)",
          "legendFormat": "cpu"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "textMode": "auto"
      }
    },
    {
      "type": "stat",
      "title": "Memory Usage (avg)",
      "datasource": "Prometheus",
      "gridPos": {
        "x": 18,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "avg(unpoller_unifi_device_mem_used / unpoller_unifi_device_mem_total * 100)",
          "legendFormat": "memory"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "textMode": "auto"
      }
    },
    {
      "type": "timeseries",
      "title": "Client Throughput",
      "datasource": "Prometheus",
      "gridPos": {
        "x": 0,
        "y": 4,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "sum(rate(unpoller_unifi_client_rx_bytes[5m]))",
          "legendFormat": "rx"
        },
        {
          "expr": "sum(rate(unpoller_unifi_client_tx_bytes[5m]))",
          "legendFormat": "tx"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "Bps",
          "min": 0
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Device Uptime (min)",
      "datasource": "Prometheus",
      "gridPos": {
        "x": 12,
        "y": 4,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "min(unpoller_unifi_device_uptime)",
          "legendFormat": "uptime"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "min": 0
        },
        "overrides": []
      }
    }
  ]
}
